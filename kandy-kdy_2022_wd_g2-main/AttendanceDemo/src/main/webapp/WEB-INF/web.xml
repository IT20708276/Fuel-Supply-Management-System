<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">




<servlet>
<servlet-name>slip</servlet-name>
<servlet-class>com.employee.LoginServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>slip</servlet-name>
<url-pattern>/log</url-pattern>
</servlet-mapping>

<servlet>

<servlet-name>slipinsert</servlet-name>
<servlet-class>com.employee.employeeinsert</servlet-class>

</servlet>

<servlet-mapping>
<servlet-name>slipinsert</servlet-name>
<url-pattern>/insert</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>updateslip</servlet-name>
<servlet-class>com.employee.UpdateSlipServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>updateslip</servlet-name>
<url-pattern>/update</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>deleteslip</servlet-name>
<servlet-class>com.employee.DeleteSlipServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>deleteslip</servlet-name>
<url-pattern>/delete</url-pattern>
</servlet-mapping>


</web-app>
